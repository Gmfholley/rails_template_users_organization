<p id="notice"><%= notice %></p>

<h1>Listing Organizations Users</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @organizations_users.each do |organizations_user| %>
      <tr>
        <td><%= link_to 'Show', organizations_user %></td>
        <td><%= link_to 'Edit', edit_organizations_user_path(organizations_user) %></td>
        <td><%= link_to 'Destroy', organizations_user, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

